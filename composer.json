{
    "name": "opheus2/laravel-countries",
    "description": "Get informations about countries and their currency.",
    "keywords": [
        "laravel",
        "countries"
    ],
    "homepage": "https://github.com/opheus2/laravel-countries",
    "license": "MIT",
    "authors": [
        {
            "name": "David Ominiabohs",
            "email": "ominiabohsdavid@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0|^12.0",
        "mockery/mockery": "^1.5|^1.6",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Orpheus\\LaravelCountries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orpheus\\LaravelCountries\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "compile-data-source": "php ./scripts/compile-data-source.php"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orpheus\\LaravelCountries\\ServiceProvider"
            ],
            "aliases": {
                "Countries": "Orpheus\\LaravelCountries\\Facades\\Countries"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
